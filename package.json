{
  "name": "dotfiles",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint . --cache --fix",
    "test": "npm run lint",
    "posttest": "npm run format",
    "format": "prettier --log-level warn --ignore-unknown --write \"**/*.{js,cjs,md,json,sh}\" \"bin/*\""
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "avif": "^0.5.3",
    "glob": "^11.0.0",
    "ky": "^1.7.2",
    "pretty-bytes-cli": "^3.0.0",
    "prompts": "^2.4.2",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "strip-json-comments": "^5.0.1",
    "tempy": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "eslint-config-tamia": "^9.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-sh": "^0.18.0"
  }
}
